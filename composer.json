{
    "name": "nswdpc/silverstripe-inline-linker",
    "description": "Inline Link field that saves to gorriecoe/link",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "linkfield",
        "link",
        "inline link editing"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "James",
            "role" : "Developer"
        }
    ],
    "require": {
        "gorriecoe/silverstripe-link": "^1.0.0",
        "fromholdio/silverstripe-externalurlfield" : "^1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "extra": {
        "expose": [
            "client/static/styles"
        ]
    }
}
